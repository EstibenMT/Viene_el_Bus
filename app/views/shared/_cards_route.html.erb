<div class="card box p-2" style="background-image: url(<%= route.image %>); background-size: cover; background-position: center; background-repeat: no-repeat">
  <div class="card-body m-2" style="background-image: linear-gradient(rgb(236, 240, 241), rgba(236, 240, 241, 0.7))">
    <h4 class="text-break card-title h-30"><%= route.name %></h4>
      <p class="text-break h-30"><%= route.price %></p>
      <p class="text-break h-10"><%= route.code %></p>
  </div>
</div>
